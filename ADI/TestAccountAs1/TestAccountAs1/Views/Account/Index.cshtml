@model IEnumerable<TestAccountAs1.AccountServiceReference.AccountServiceClient>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            AccountNam
        </th>
        <th>
           AccountNumber
        </th>
        <th>
            model.CMND
        </th>
        <th>
            CodePin
        </th>
        <th>
          Created
        </th>
        <th>
            Money
        </th>
        <th></th>
    </tr>

@foreach (TestAccountAs1.Models.Account item in ViewBag.listAccounts) {
    <tr>
        <td>
            @item.AccountName
        </td>
        <td>
            @item.AccountNumber
        </td>
        <td>
            @item.CMND
        </td>
        <td>
            @item.CodePin
        </td>
        <td>
            @item.Created
        </td>
        <td>
            @item.Money
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = @item.AccountNumber }) |
            @Html.ActionLink("Details", "Details", new { id= @item.AccountNumber }) |
            @Html.ActionLink("Delete", "Delete", new { id = @item.AccountNumber })
        </td>
    </tr>
}

</table>
